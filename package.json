    "knex": "^2.5.1",
    "node-xlsx": "^0.23.0",
    "pg": "^8.11.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
