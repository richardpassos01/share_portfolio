{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc && tsc-alias",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "test": "jest  --config jest.config.ts",
    "start": "node build/src/api/server.js",
    "predevelopment": "npm run migration:up",
    "development": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify -r dotenv/config src/api/server.ts",
    "knex": "node -r dotenv/config ./node_modules/knex/bin/cli.js",
    "migration:create": "npm run knex migrate:make",
    "migration:up": "npm run knex migrate:latest",
    "migration:down": "npm run knex migrate:rollback",
    "seed:create": "npm run knex seed:make",
    "seed:run": "npm run knex seed:run",
    "management": "node -r dotenv/config src/management/commands/${npm_config_command}.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.1",
    "joi": "^17.10.2",
    "knex": "^2.5.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "node-xlsx": "^0.23.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.13.9",
    "@types/koa__cors": "^4.0.1",
    "@types/koa__router": "^12.0.1",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/node": "^20.8.2",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.14",
    "@types/uuidv4": "^5.0.0",
    "gts": "^5.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
